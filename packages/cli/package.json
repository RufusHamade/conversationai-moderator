{
  "name": "@conversationai/moderator-cli",
  "description": "Commands for OSMod",
  "version": "1.0.6",
  "scripts": {
    "lint": "find commands -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "lint:fix": "find commands -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "build": "npm run compile",
    "compile": "../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration",
    "watch": "../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --watch"
  },
  "bin": {
    "osmod": "./bin/osmod.js"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": "6.11.0"
  },
  "dependencies": {
    "@conversationai/moderator-backend-core": "1.0.6",
    "@conversationai/moderator-config": "1.0.3",
    "@types/axios": "0.14.0",
    "@types/js-yaml": "3.5.29",
    "@types/lodash": "4.14.58",
    "@types/mysql": "0.0.31",
    "@types/request": "0.0.42",
    "@types/sequelize": "3.4.43",
    "@types/yargs": "6.6.0",
    "axios": "0.15.3",
    "find-up": "2.1.0",
    "js-yaml": "3.8.2",
    "lodash": "4.17.4",
    "mysql": "2.13.0",
    "request": "2.81.0",
    "sequelize": "3.30.4",
    "sequelize-cli": "2.7.0",
    "yargs": "7.0.2",
    "@types/bluebird": "3.5.2",
    "bluebird": "3.5.0"  
  }
}
