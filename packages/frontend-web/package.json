{
  "name": "@conversationai/moderator-frontend-web",
  "version": "1.1.0",
  "main": "dist-commonjs/index.js",
  "types": "./dist-commonjs/index.d.ts",
  "description": "OSMod project React.js frontend",
  "scripts": {
    "build:webpack": "npm run compile:lib && NODE_ENV=production webpack --bail --progress --profile --colors --config tooling/webpack.config.production.js",
    "build:copy": "cp -R public/* build/",
    "build": "npm run compile:web && npm run build:webpack && npm run storybook:build && npm run build:copy",
    "lint": "find src -name *.ts -o -name *.tsx | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "watch": "PORT=8000 API_URL=http://localhost:8080 webpack-dev-server --config tooling/webpack.config.js --progress --colors --host 0.0.0.0",
    "lint:fix": "find src -name *.ts -o -name *.tsx | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "test": "npm run spec && npm run storybook:test",
    "spec": "jest dist-commonjs/**",
    "test:watch": "npm run compile:lib && npm run compile:lib:watch | jest --watch",
    "storybook": "npm run compile && npm run compile:watch | start-storybook --dont-track -p 9001 -c tooling/storybook -s public",
    "storybook:build": "build-storybook -c tooling/storybook -s public -o build/storybook",
    "storybook:test": "jest tooling/storybook/Storyshots.test.js",
    "compile": "npm run compile:web | npm run compile:lib",
    "compile:web": "rm -rf dist && ../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --importHelpers",
    "compile:lib": "rm -rf dist-commonjs && ../../node_modules/.bin/tsc --sourceMap --outDir dist-commonjs --declaration --module commonjs && npm run compile:lib:cleanup",
    "compile:lib:cleanup": "find dist-commonjs -type f -exec sed -i -e 's/_1\\.default(/_1(/g' {} \\; && find dist-commonjs -type f -exec sed -i -e 's/_1\\.default\\./_1\\./g' {} \\;",
    "compile:lib:watch": "../../node_modules/.bin/tsc --sourceMap --outDir dist-commonjs --declaration --module commonjs --watch",
    "compile:watch": "../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --importHelpers --watch --preserveWatchOutput",
    "start": "node tooling/server.js"
  },
  "license": "Apache-2.0",
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "@conversationai/moderator-config": "1.1.0",
    "@conversationai/moderator-jsonapi": "1.1.0",
    "@types/express": "4.11.1",
    "@types/faker": "4.1.4",
    "express": "4.16.3",
    "faker": "4.1.0",
    "immutable": "3.8.1",
    "typed-immutable-record": "0.0.6"
  },
