    "@storybook/cli": "3.4.11",
    "@storybook/react": "3.4.11",
    "@types/aphrodite": "0.5.7",
    "@types/check-types": "7.3.0",
    "@types/enzyme": "3.1.15",
    "@types/keyboardjs": "2.4.0",
